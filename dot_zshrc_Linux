export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$HOME/local/lib

# ls
alias ls='ls --color=auto -F'
zstyle ':completion:*:default' list-colors ${(s.:.)LS_COLORS}

# command-not-found
if [[ -f /etc/zsh_command_not_found ]] then
  source /etc/zsh_command_not_found
fi

# For GPG Settings
export GPG_TTY=$(tty)
